<div class="con-med border border-primary rounded p-3">
  <div class="text-center">
    <h1 class="text-center mb-4">Profile</h1>
  </div>
<form action="<%= user_user_profiles_path %>" method="GET">
<input type="hidden" value="<%= form_authenticity_token %>" name= "authenticity_token" />
<div class="form-row">
  <div class="form-group col-md-6">
    <%= form_for @user_profile, url: {action: "update"} do |f| %>
    <b><%= f.label :First_Name %></b>
    <%= f.text_field :first_name, class: 'form-control' %>
  </div>
  <div class="form-group col-md-6">
    <b><%= f.label :Last_Name %></b>
    <%= f.text_field :last_name, class: 'form-control' %>
  </div>
</div>
<div class="form-group">
  <b><%= f.label :Street %></b>
  <%= f.text_field :street, class: 'form-control'%>
</div>
<div class="form-row">
  <div class="form-group col-md-6">
    <b><%= f.label :Suburb %></b>
    <%= f.text_field :suburb, class: 'form-control' %>
  </div>
  <div class="form-group col-md-6">
    <b><%= f.label :Postcode %></b>
    <%= f.text_field :postcode, class: 'form-control' %>
  </div>
</div>
  <div class="form-group">
    <b><%= f.label :State %></b>
    <%= f.text_field :state, class: 'form-control' %>
  </div>
  <div class="form-group">
    <b><%= f.label :Country %></b>
    <%= f.text_field :country, class: 'form-control' %>
  </div>
  <%= f.submit 'Save Changes', class:'btn btn-primary' %>
  <% end %>
 
  